{
  "name": "coolmut/yandex",
  "description": "PHP SDK для работы с некоторыми сервисами яндекса (Яндекс.Диск, Yandex.Disk)",
  "keywords": ["yandex", "disk", "диск", "яндекс", "php", "curl", "api", "client", "sdk",
    "yandex disk", "яндекс диск", "rest", "restful", "яндекс"],
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitry Arhitector",
      "email": "dmitry.arhitector@yandex.ru"
    }
  ],
  "require": {
    "php": ">=7.3",
    "laminas/laminas-diactoros": "^2.5",
    "league/event": "^2.2",
    "php-http/curl-client": "2.*",
    "php-http/message": "^1.2",
    "php-http/client-common": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4"
  },
  "autoload": {
    "psr-4": {
      "Coolmut\\Yandex\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Coolmut\\Yandex\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
